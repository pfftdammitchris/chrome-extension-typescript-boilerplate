{
  "// @ts-ignore": {
    "prefix": "tsg",
    "body": "// @ts-ignore"
  },
  "@param": {
    "prefix": "@param",
    "body": ["/**", " * @param { $1 } $2 - $3", " */"]
  },
  "beforeEach(() => {})": {
    "prefix": "bfe",
    "body": ["beforeEach(() => {", "  $1", "})"]
  },
  "comment section": {
    "prefix": "cs",
    "body": [
      "/* -------------------------------------------------------",
      "  ---- $1",
      "-------------------------------------------------------- */"
    ]
  },
  "describe": {
    "prefix": "desc",
    "body": ["describe('$1', () => {", "  $2", "})"]
  },
  "it": {
    "prefix": "it",
    "body": ["it('$1', () => {", "  $2", "})"]
  },
  "document.querySelector": {
    "prefix": "qs",
    "body": "const $1 = document.querySelector('$2')"
  },
  "document.getElementsByClassName": {
    "prefix": "getclass",
    "body": "const $1 = document.getElementsByClassName('$2')"
  },
  "document.getElementById": {
    "prefix": "getid",
    "body": "const $1 = document.getElementById('$2')"
  },
  "eslint disable next line": {
    "prefix": "ednl",
    "body": "// eslint-disable-next-line"
  },
  "eslint disable line": {
    "prefix": "eds",
    "body": "// eslint-disable-line"
  }
}
